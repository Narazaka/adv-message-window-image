    "watch": "webpack -w",
    "firebase:deploy": "firebase deploy",
    "deploy": "npm-run-all build:production firebase:deploy",
    "png:minify": "npm-run-all png:pngquant png:advpng",
    "png:pngquant": "pngquant --speed 1 --strip --ext .png --force public/*.png public/**/*.png",
    "png:advpng": "advpng -z -4 -i 10 public/*.png public/**/*.png",
    "convfont": "npm-run-all convfont:farewell_school_title convfont:farewell_school_body",
    "convfont:farewell_school_title": "node scripts/convert_color.js public/vl_pgothic_white_30 public/vl_pgothic_farewell_school_title vl_pgothic.fnt 255 186 179",
    "convfont:farewell_school_body": "node scripts/convert_color.js public/vl_pgothic_white_30 public/vl_pgothic_farewell_school_body vl_pgothic.fnt 119 102 102"
  },
